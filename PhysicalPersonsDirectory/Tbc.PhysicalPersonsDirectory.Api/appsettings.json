{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ImagesOptions": {
    "MaxImageSizeBytes": 4194304, //4 * 1024 * 1024; 4MB
    "AllowedImageExtensions": [
      ".jpg",
      ".jpeg",
      ".png",
      ".webp",
      ".bmp" //სხვა არაფერი მახსენდება ალექსანდრე, თუ რამე სხვა გაფართოების სურათებს ატვირთავ დავამატოთ :D
    ]
  },
  "PageOptions": {
    "PageSize": 10
  },
  "LocalizationCustomOptions": {
    "DefaultCulture": "en-US",
    "SupportedCultures": [
      "en-US",
      "ka-GE"
    ]
  },
  "ConnectionStrings": {
    "PhysicalPersonsContext": "Data Source=localhost;Database=PhysicalPersonsDb;Integrated Security=True;Trust Server Certificate=True"
  }
}